{
  "name": "lavacao-detailers",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "Calia Dalari",
    "email": "cdalari@gmail.com"
  },
  "description": "Aplicativo para Controle de Serviços",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@formatjs/intl": "^2.9.6",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.15.4",
    "@mui/joy": "^5.0.0-beta.15",
    "@mui/material": "^5.14.17",
    "@mui/styled-engine-sc": "^6.0.0-alpha.5",
    "@mui/styles": "^5.14.17",
    "@mui/system": "^5.14.17",
    "@mui/x-data-grid": "^6.18.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.1",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-currency-input-field": "^3.6.12",
    "react-dom": "^18.2.0",
    "react-input-mask": "^2.0.4",
    "react-ios-switch": "^0.1.19",
    "react-number-format": "^5.3.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-spring": "^9.7.3",
    "styled-components": "^6.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron:package:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "extraMetadata": {
      "main": "public/electron.js"
    },
    "appId": "com.exemplo.calian",
    "productName": "Lavação Detailers",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "public/detailer-logo-256.png"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^25.4.0",
    "electron-builder": "24.6.3",
    "web-vitals": "^3.4.0"
  }
}
